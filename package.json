{
  "name": "kawajs",
  "version": "2.3.0",
  "description": "React/Redux based framework",
  "author": "Paul Reboh",
  "license": "MIT",
  "keywords": [
    "react",
    "redux"
  ],
  "homepage": "https://github.com/somelibs/kawajs",
  "repository": {
    "type": "git",
    "url": "https://github.com/somelibs/kawajs.git"
  },
  "bugs": {
    "url": "https://github.com/somelibs/kawajs/issues"
  },
  "files": [
    "dist/cjs/**",
    "dist/es/**"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir dist/cjs",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es",
    "commonjs:watch": "yarn build:commonjs -- --watch",
    "es:watch": "yarn build:es -- --watch",
    "build": "yarn build:commonjs && yarn build:es",
    "watch": "yarn commonjs:watch & yarn es:watch",
    "dev": "yarn watch & yarn lint:watch",
    "lint": "eslint src tests --fix",
    "lint:watch": "nodemon -e js -x 'yarn lint'",
    "test": "jest --noStackTrace",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-syntax-export-default-from": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "babel-jest": "^29.5.0",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "nodemon": "^2.0.22",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.14",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "aphrodite": "^2.3.1",
    "classnames": "^2.3.2",
    "clean-deep": "^3.4.0",
    "core-js": "^3.30.0",
    "escape-regex-string": "^1.0.6",
    "history": "^4.10.1",
    "lodash": "^4.17.20",
    "merge": "^2.1.1",
    "prop-types": "^15.8.1",
    "query-string": "^6.14.1",
    "react-is": "^16.13.1",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "reselect": "^5.1.1",
    "uuid": "^9.0.0",
    "warning": "^4.0.3"
  }
}
